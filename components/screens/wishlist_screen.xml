<?xml version="1.0" encoding="utf-8" ?>
<component name="wishlist_screen" extends="Group">
  <interface>
    <field id="wishlist_finished" type="string" alwaysnotify = "true" alias = "wishlist_video.state"  />
  </interface>
  <script type="text/brightscript" uri="pkg:/components/screens/wishlist_screen.brs" />
  <children>
   <Video
        id="wishlist_video"
        visible="false"
        translation="[0,0]"
        width="1920"
        height="1080"
        enableUI = "false" />
   <Label
      id="wishlist_title"
      text="wishlist"
      translation="[750, 200]"
      font="font:Outfit-Bold">
    <Font role="font" uri = "pkg:/fonts/Outfit-Bold.ttf" size="500"/>
   </Label>
  </children>
</component>